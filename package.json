{
  "name": "google-index-tools",
  "version": "1.5.0-alpha.2",
  "main": "dist/main/index.js",
  "private": true,
  "productName": "Google Index Tools",
  "keywords": [
    "indexing-api",
    "google",
    "electron",
    "vite",
    "vue3",
    "vue"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344/"
    }
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && electron-builder --mac",
    "build:win": "vue-tsc --noEmit && vite build && electron-builder --win portable",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.ts,.tsx,.vue src --fix"
  },
  "lint-staged": {
    "*.{js,ts,tsx,css,json,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^6.6.0",
    "@unocss/preset-icons": "^0.55.4",
    "@unocss/preset-uno": "^0.55.4",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mrm": "^4.1.17",
    "naive-ui": "^2.34.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "unocss": "^0.55.7",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue-tsc": "^1.8.8"
  },
  "dependencies": {
    "@electron/remote": "^2.0.11",
    "@intlify/unplugin-vue-i18n": "^1.0.1",
    "@unocss/preset-attributify": "^0.55.7",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vueuse/core": "^10.4.1",
    "date-fns": "^2.30.0",
    "googleapis": "^126.0.1",
    "ofetch": "^1.3.3",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "9",
    "vue-json-pretty": "^2.2.4",
    "vue-router": "^4.2.4",
    "winston": "^3.10.0",
    "yup": "^1.2.0"
  },
  "volta": {
    "node": "20.6.1",
    "pnpm": "7.33.6"
  }
}
